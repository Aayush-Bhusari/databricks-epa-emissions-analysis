{
  "datasets": [
    {
      "name": "44a3b754",
      "displayName": "Location Data",
      "queryLines": [
        "SELECT latitude,\r\n",
        "longitude,\r\n",
        "`GHG emissions mtons CO2e` as Emissions\r\n",
        "FROM emissions.default.emissions_data"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "31a45748",
      "displayName": "Emissions per Person",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(\r\n",
        "        REPLACE(`GHG emissions mtons CO2e`, ',' , '')\r\n",
        "        AS DOUBLE\r\n",
        "        ) / NULLIF(\r\n",
        "          CAST(\r\n",
        "            REPLACE(population, ',', '') \r\n",
        "            AS DOUBLE\r\n",
        "          ),0\r\n",
        "        ) AS Emission_per_person \r\n",
        "FROM emissions_data\r\n",
        "ORDER BY emission_per_person DESC\r\n"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "ec809459",
      "displayName": "Total Emissions per State",
      "queryLines": [
        "SELECT state_abbr,\r\n",
        "       SUM(CAST(\r\n",
        "        REPLACE(`GHG emissions mtons CO2e`, ',' , '')\r\n",
        "        AS DOUBLE\r\n",
        "      ))AS Total_Emissions \r\n",
        "FROM emissions_data\r\n",
        "GROUP BY state_abbr\r\n",
        "ORDER BY total_emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "c459ff18",
      "displayName": "County Shaming",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(\r\n",
        "        REPLACE(`GHG emissions mtons CO2e`, ',' , '')\r\n",
        "        AS DOUBLE) AS Total_Emissions\r\n",
        "FROM emissions_data\r\n",
        "ORDER BY Total_Emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
